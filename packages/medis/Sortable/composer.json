{
    "name": "medis/Sortable",
    "description": "Sort Eloquent models using Sortable javascript library",
    "type": "library",
    "authors": [
        {
            "name": "medis",
            "email": "aubuka@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "secure-http": false,
        "discard-changes": true
    },
    "require": {
        "composer/installers": "^1.0.20",
        "components/rubaxa-sortable": "1.6.0"
    },
    "autoload": {
        "psr-4": {
            "medis\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "medis\\Sortable\\SortableServiceProvider"
            ]
        },
        "installer-paths" : {
            "libraries/{$name}" : [
                "type:library"
            ]
        }
    }
}
